<mujoco model="lwr_with_wsg50">

    <actuator>
        <!-- Physical limits of the actuator. -->
        <!-- Values taken from kuka_lbr_iiwa_brochure_en.pdf (see website) -->
        <motor ctrllimited="true" ctrlrange="-176.0 176.0" joint="joint_1" name="torq_j1"/>
        <motor ctrllimited="true" ctrlrange="-176.0 176.0" joint="joint_2" name="torq_j2"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_3" name="torq_j3"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_4" name="torq_j4"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_5" name="torq_j5"/>
        <motor ctrllimited="true" ctrlrange="-40.0 40.0" joint="joint_6" name="torq_j6"/>
        <motor ctrllimited="true" ctrlrange="-40.0 40.0" joint="joint_7" name="torq_j7"/>

        <position ctrllimited="true" ctrlrange="-0.05 0" joint="finger_joint1" kp="1000" name="finger_j1" forcelimited="true" forcerange="-20 20"/>
        <position ctrllimited="true" ctrlrange="0 0.05" joint="finger_joint2" kp="1000" name="finger_j2" forcelimited="true" forcerange="-20 20"/>
    </actuator>

    <asset>
         <!-- coliision part for robot model -->
        <!-- <mesh name="arm_base_convex" file="meshes/arm_base_convex.stl" />
        <mesh name="arm_segment_a_convex" file="meshes/arm_segment_a_convex.stl" />
        <mesh name="arm_segment_b_convex" file="meshes/arm_segment_b_convex.stl" />
        <mesh name="arm_segment_last" file="meshes/arm_segment_last.stl" />
        <mesh name="arm_wrist_convex" file="meshes/arm_wrist_convex.stl" /> -->
       <!-- visual part for robot model -->
        <mesh name="arm_base" file="meshes/arm_base.stl" />
        <mesh name="arm_segment_a" file="meshes/arm_segment_a.stl" />
        <mesh name="arm_segment_b" file="meshes/arm_segment_b.stl" />
        <mesh name="arm_wrist" file="meshes/arm_wrist.stl" />
        <mesh name="arm_segment_last" file="meshes/arm_segment_last.stl" />
        <!-- visual part for WSG50 model -->
        <mesh name="WSG50_110" file="meshes/WSG50_110.stl" />
        <mesh name="GUIDE_WSG50_110" file="meshes/GUIDE_WSG50_110.stl" scale="0.001 0.001 0.001" />
        <mesh name="WSG-DSA" file="meshes/WSG-DSA.stl" scale="0.001 0.001 0.001" />

    </asset>
    <worldbody>
        <body name="base" pos="0 0 0">
        <!-- mount attached here -->
            <!-- <geom size="1.3 1 0.4" pos="-0.31 -0.19 0.4" quat="0.707107 0 0 -0.707107" type="box" rgba="1 1 1 1" /> -->
            <!-- <geom size="1.5 0.5 1" pos="-0.81 -0.59 1.1" quat="0.707107 0 0 -0.707107" type="box" rgba="0.2 0.2 0.2 1" />
            <geom size="0.75 0.5 1" pos="0.2 -1.25 1.1" quat="0.707107 0 0 -0.707107" type="box" rgba="0.2 0.2 0.2 1" /> -->
            <geom size="0.1375 0.1375 0.007" pos="0 0 0.8071" type="box" conaffinity="0" group="1" rgba="0.6 0.6 0.6 1" name="robot_table_adapter"/>
            <geom pos="0 0 0.8142" type="mesh" rgba="1 0.487 0 1" conaffinity="0" group="1" mesh="arm_base" name="base_visual"/>

            <body name="lwr_arm_1_link" pos="0 0 0.9242">
                <inertial pos="0 -0.06 0.13" mass="2" diaginertia="0.1 0.1 0.1" />
                <joint name="joint_1" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                <geom type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_a" name="link_1_visual"/>

                <body name="lwr_arm_2_link" pos="0 0 0.2" quat="0.707107 -0.707107 0 0">
                    <inertial pos="0 0.06 0.07" mass="2" diaginertia="0.1 0.1 0.1" />
                    <joint name="joint_2" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" frictionloss="10" />
                    <geom pos="0 -0.2 0" quat="0.707107 -0.707107 0 0" type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_b" name="link_2_visual" />

                    <body name="lwr_arm_3_link" pos="0 -0.2 0" quat="0.707107 0.707107 0 0">
                        <inertial pos="0 0.06 0.13" mass="2" diaginertia="0.1 0.1 0.1" />
                        <joint name="joint_3" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                        <geom quat="0 0 0 1" type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_a" name="link_3_visual"/>

                        <body name="lwr_arm_4_link" pos="0 0 0.2" quat="0.707107 0.707107 0 0">
                            <inertial pos="0 -0.06 0.07" mass="2" diaginertia="0.1 0.1 0.1" />
                            <joint name="joint_4" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" />
                            <geom pos="0 0.2 0" quat="0 0 0.707107 -0.707107" type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_b" name="link_4_visual"/>

                            <body name="lwr_arm_5_link" pos="0 0.2 0" quat="0.707107 -0.707107 0 0">
                                <inertial pos="0 0 0.124" mass="2" diaginertia="0.1 0.1 0.1" />
                                <joint name="joint_5" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                                <geom type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_last" name="link_5_visual" />

                                <body name="lwr_arm_6_link" pos="0 0 0.19" quat="0.707107 -0.707107 0 0">
                                    <inertial pos="0 0 0.0625" mass="0.2" diaginertia="0.1 0.1 0.1" />
                                    <joint name="joint_6" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" />
                                    <geom quat="0.707107 0.707107 0 0" type="mesh" conaffinity="0" group="1" rgba="0.3 0.3 0.3 1" mesh="arm_wrist" name="link_6_visual"/>

                                    <body name="lwr_arm_7_link" pos="0 -0.078 0" quat="0.707107 0.707107 0 0">
                                        <inertial pos="4.13636e-05 -0.00367545 0.00886364" quat="0.970613 0.240578 0.00135371 0.00546148" mass="0.55" diaginertia="4.00003 4.00003 4" />
                                        <joint name="joint_7" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                                        <geom size="0.0315 0.01" pos="0 0 -0.01" quat="0.92388 0 0 -0.382683" type="cylinder" conaffinity="0" group="1" rgba="0.3 0.3 0.3 1" name="link_7_visual" />
                                        <geom size="0.0315 0.005" pos="0 0 0.005" type="cylinder" rgba="0 0 0 1" conaffinity="0" group="1" name="robot_top_cylinder" />

                                        <body name="eef_gripper">   
                                            <!-- mount attached here (gripper) -->
                                            <site name="ft_frame" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 1" type="sphere" group="1" />
                                            <!-- This site was added for visualization. -->
                                            <body name="eef" pos="0.0 -0.01 0.15" quat="0 1 0 0">
                                                <site name="grip_site" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 0.5" type="sphere" group="1"/>
                                                <site name="ee_x" pos="0.1 0 0" size="0.005 .1"  quat="0.707105  0 0.707108 0 " rgba="1 0 0 0" type="cylinder" group="1"/>
                                                <site name="ee_y" pos="0 0.1 0" size="0.005 .1" quat="0.707105 0.707108 0 0" rgba="0 1 0 0" type="cylinder" group="1"/>
                                                <site name="ee_z" pos="0 0 0.1" size="0.005 .1" quat="1 0 0 0" rgba="0 0 1 0" type="cylinder" group="1"/>
                                                <!-- This site was added for visualization. -->
                                                <site name="grip_site_cylinder" pos="0 0 0" size="0.005 10" rgba="0 1 0 0.3" type="cylinder" group="1"/>
                                            </body>    
                                            <geom size="0.06 0.03 0.0025" pos="0 0 0.0125" quat="0.707107 0 0 -0.707107" type="box" conaffinity="0" group="1"  rgba="0.6 0.6 0.6 1" name="robot_gripper_adapter"/>
                                            <geom pos="9.1e-05 -0.008086 0.015" quat="0.707107 0 0 -0.707107" type="mesh" conaffinity="0" group="1"  mesh="WSG50_110" name="gripper_palm"/>
                                            <body name="gripper_left" pos="9.1e-05 -0.008086 0.015" quat="0.707107 0 0 -0.707107">
                                                <inertial pos="-0.0045 0 0.0408" quat="0.998492 0 -0.0548974 0" mass="0.2" diaginertia="2.00034 2.00034 2" />
                                                <joint name="finger_joint1" pos="0 0 0" axis="1 0 0" type="slide" limited="true" range="-0.055 -0.0027" damping="1" frictionloss="1" />
                                                <geom type="mesh" rgba="0 0 0 1" name = 'left_finger_collision' conaffinity="0" group="1"  mesh="GUIDE_WSG50_110" />
                                                <geom pos="-0.009 0 0.0816" type="mesh" rgba="0 0 0 1" name ='left_finger_pad' conaffinity="0" group="1" friction="1 0.005 0.9" mesh="WSG-DSA" />
                                            </body>
                                            <body name="gripper_right" pos="9.1e-05 -0.008086 0.015" quat="0.707107 0 0 0.707107">
                                                <inertial pos="-0.0045 0 0.0408" quat="0.998492 0 -0.0548974 0" mass="0.2" diaginertia="2.00034 2.00034 2" />
                                                <joint name="finger_joint2" pos="0 0 0" axis="-1 0 0" type="slide" limited="true" range="0.0027 0.055" damping="1" frictionloss="1" />
                                                <geom type="mesh" rgba="0 0 0 1" name = 'right_finger_collision' conaffinity="0" group="1"  mesh="GUIDE_WSG50_110" />
                                                <geom pos="-0.009 0 0.0816" type="mesh" rgba="0 0 0 1" name ='right_finger_pad' conaffinity="0" group="1" friction="1 0.005 0.9" mesh="WSG-DSA" />
                                            </body>
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <sensor>
        <!-- get force and torque of end-effector sim.data.sensordata dim:7 -->
        <force name="force_ee" site="ft_frame"/>
        <torque name="torque_ee" site="ft_frame"/>
    </sensor>
</mujoco>
