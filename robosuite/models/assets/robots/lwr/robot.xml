<mujoco model="lwr">
    <actuator>
        <!-- Physical limits of the actuator. -->
        <!-- Values taken from kuka_lbr_iiwa_brochure_en.pdf (see website) -->
        <motor ctrllimited="true" ctrlrange="-176.0 176.0" joint="joint_1" name="torq_j1"/>
        <motor ctrllimited="true" ctrlrange="-176.0 176.0" joint="joint_2" name="torq_j2"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_3" name="torq_j3"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_4" name="torq_j4"/>
        <motor ctrllimited="true" ctrlrange="-110.0 110.0" joint="joint_5" name="torq_j5"/>
        <motor ctrllimited="true" ctrlrange="-40.0 40.0" joint="joint_6" name="torq_j6"/>
        <motor ctrllimited="true" ctrlrange="-40.0 40.0" joint="joint_7" name="torq_j7"/>
    </actuator>
    <asset>
        <mesh name="arm_base_convex" file="meshes/arm_base_convex.stl" />
        <mesh name="arm_segment_a_convex" file="meshes/arm_segment_a_convex.stl" />
        <mesh name="arm_segment_b_convex" file="meshes/arm_segment_b_convex.stl" />
        <mesh name="arm_segment_last" file="meshes/arm_segment_last.stl" />
        <mesh name="arm_wrist_convex" file="meshes/arm_wrist_convex.stl" />

        <mesh name="arm_base" file="meshes/arm_base.stl" />
        <mesh name="arm_segment_a" file="meshes/arm_segment_a.stl" />
        <mesh name="arm_segment_b" file="meshes/arm_segment_b.stl" />      
        <mesh name="arm_wrist" file="meshes/arm_wrist.stl" />
    </asset>
    <worldbody>
        <body name="base" pos="0 0 0">
                <!-- robot view -->
                <camera mode="fixed" name="robotview" pos="1.0 0 1.5" quat="0.653 0.271 0.271 0.653"/>
                <inertial diaginertia="0 0 0" mass="0" pos="0 0 0"/>
                <!-- mount attached here -->
                <!-- <geom size="1.3 1 0.4" pos="-0.31 -0.19 0.4" quat="0.707107 0 0 -0.707107" type="box" rgba="1 1 1 1" /> -->
                <!-- <geom size="1.5 0.5 1" pos="-0.81 -0.59 1.1" quat="0.707107 0 0 -0.707107" type="box" rgba="0.2 0.2 0.2 1" /> -->
                <!-- <geom size="0.75 0.5 1" pos="0.2 -1.25 1.1" quat="0.707107 0 0 -0.707107" type="box" rgba="0.2 0.2 0.2 1" /> -->
                <geom size="0.1375 0.1375 0.007" pos="0 0 0.8071" type="box" rgba="0.6 0.6 0.6 1" />
                <geom pos="0 0 0.8142" type="mesh" rgba="1 0.487 0 1" conaffinity="0" group="1" name="base_visual"  mesh="arm_base" />
            
                <body name="lwr_arm_1_link" pos="0 0 0.9242">
                    <inertial pos="0 -0.06 0.13" mass="2" diaginertia="0.1 0.1 0.1" />
                    <joint name="joint_1" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                    <geom type="mesh"  conaffinity="0" group="1" rgba="1 0.487 0 1" name="link_1_visual" mesh="arm_segment_a" />
                    <!-- <geom type="mesh" rgba="1 0.487 0 1" name="link_1_collision" mesh="arm_segment_a_convex"  />  -->

                    <body name="lwr_arm_2_link" pos="0 0 0.2" quat="0.707107 -0.707107 0 0">
                        <inertial pos="0 0.06 0.07" mass="2" diaginertia="0.1 0.1 0.1" />
                        <joint name="joint_2" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" frictionloss="10" />
                        <geom pos="0 -0.2 0" quat="0.707107 -0.707107 0 0" type="mesh" conaffinity="0" group="1" rgba="1 0.487 0 1" mesh="arm_segment_b" name="link_2_visual"/>
                        <!-- <geom pos="0 -0.2 0" quat="0.707107 -0.707107 0 0" type="mesh" rgba="1 0.487 0 1" mesh="arm_segment_b_convex" name="link_2_collision" /> -->

                        <body name="lwr_arm_3_link" pos="0 -0.2 0" quat="0.707107 0.707107 0 0">
                            <inertial pos="0 0.06 0.13" mass="2" diaginertia="0.1 0.1 0.1" />
                            <joint name="joint_3" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                            <geom quat="0 0 0 1" type="mesh" rgba="1 0.487 0 1" conaffinity="0" group="1" mesh="arm_segment_a" name="link_3_visual"/>
                            <!-- <geom quat="0 0 0 1" type="mesh" rgba="1 0.487 0 1" mesh="arm_segment_a_convex" name="link_3_collision"/> -->

                            <body name="lwr_arm_4_link" pos="0 0 0.2" quat="0.707107 0.707107 0 0">
                                <inertial pos="0 -0.06 0.07" mass="2" diaginertia="0.1 0.1 0.1" />
                                <joint name="joint_4" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" />
                                <geom pos="0 0.2 0" quat="0 0 0.707107 -0.707107" type="mesh" rgba="1 0.487 0 1"  conaffinity="0" group="1" mesh="arm_segment_b" name="link_4_visual"/>
                                <!-- <geom pos="0 0.2 0" quat="0 0 0.707107 -0.707107" type="mesh" rgba="1 0.487 0 1" mesh="arm_segment_b_convex" name="link_4_collision"/> -->

                                <body name="lwr_arm_5_link" pos="0 0.2 0" quat="0.707107 -0.707107 0 0">
                                    <inertial pos="0 0 0.124" mass="2" diaginertia="0.1 0.1 0.1" />
                                    <joint name="joint_5" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                                    <geom type="mesh" rgba="1 0.487 0 1" conaffinity="0" group="1" mesh="arm_segment_last" name="link_5_visual"/>
                                    <!-- <geom type="mesh" rgba="1 0.487 0 1" mesh="arm_segment_last" name="link_5_collision"/> -->

                                    <body name="lwr_arm_6_link" pos="0 0 0.19" quat="0.707107 -0.707107 0 0">
                                        <inertial pos="0 0 0.0625" mass="0.2" diaginertia="0.1 0.1 0.1" />
                                        <joint name="joint_6" pos="0 0 0" axis="0 0 1" limited="true" range="-2.0944 2.0944" damping="0.1" />
                                        <geom quat="0.707107 0.707107 0 0" type="mesh" rgba="0.3 0.3 0.3 1" conaffinity="0" group="1" mesh="arm_wrist"  name="link_6_visual" />
                                        <!-- <geom quat="0.707107 0.707107 0 0" type="mesh" rgba="0.3 0.3 0.3 1" mesh="arm_wrist_convex" name="link_6_collision"/> -->

                                        <body name="lwr_arm_7_link" pos="0 -0.078 0" quat="0.707107 0.707107 0 0">
                                            <inertial pos="0 0 0.002" mass="0.25" diaginertia="2 2 2" />
                                            <joint name="joint_7" pos="0 0 0" axis="0 0 1" limited="true" range="-2.96706 2.96706" damping="0.1" />
                                            <geom size="0.0315 0.01" pos="0 0 -0.01" quat="0.92388 0 0 -0.382683" type="cylinder" rgba="0.3 0.3 0.3 1" conaffinity="0" group="1"/>
                                            <geom size="0.0315 0.005" pos="0 0 0.005" type="cylinder" rgba="0 0 0 1" />

                                            <body name="right_hand" pos="0 0 0.044" quat="0 0 0 1">
                                                <!-- This sites were added for visualization. They are all standardized between models-->
                                                <!-- Position mimics the gripper attachment point (right_hand) -->
                                                <!--  Y-axis should be perpendicular to grasping motion, and Z-axis should point out of the robot eef -->
                                                <site name="ee" pos="0 0 0" size="0.01 0.01 0.01" rgba="0 0 1 1" type="sphere" group="1"/>
                                                <site name="ee_x" pos="0 0 0" size="0.005 .1"  quat="0.707105 0.707108 0 0 " rgba="1 0 0 0" type="cylinder" group="1"/>
                                                <site name="ee_z" pos="0 0 0" size="0.005 .1" quat="0.707105 0 0 0.707108" rgba="0 0 1 0" type="cylinder" group="1"/>
                                                <site name="ee_y" pos="0 0 0" size="0.005 .1" quat="0.707105 0 0.707108 0 " rgba="0 1 0 0" type="cylinder" group="1"/>
                                                <!-- This camera points out from the eef. -->
                                                <camera mode="fixed" name="eye_in_hand" pos="0.05 0 0" quat="0 0.707108 0.707108 0" fovy="75"/>
                                                <!-- to add gripper -->
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
</mujoco>
